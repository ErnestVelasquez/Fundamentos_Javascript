const animal = {
  color: "Verde",
  nombre: "perico",
  volando: true
}

// Destructuring objetos literales...

const {color, nombre, volando} = animal;

console.log("El " + nombre + " es de color: " + color);

//Arrays indexados
const frutas = ["Pera", "Manzana", "Naranja"];


//Destructuring array
const [pera, manzana, naranja] = frutas;
console.log("El " + nombre + " come " + pera);
